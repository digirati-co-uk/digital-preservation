{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "HealthChecks": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext} {CorrelationId}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Storage-AWS": {
    "Profile": "leeds",
    "Region": "eu-west-1"
  },
  "AWS": {
    "Profile": "leeds",
    "Region": "eu-west-1"
  },
  "AllowedHosts": "*",
  "FeatureFlags": {
    "DisableAuth": "true",
    "UseLocalHostedServiceForPipeline": "true"
  },
  "PipelineJob": {
    "PipelineJobTopicArn": "arn:aws:sns:eu-west-1:975050260954:dlip-pres-local-pipeline-job-runner-topic"
  },
  "ApiKeyOptions": {
    "ApiKey": "API Key",
    "ApiHeaderName": "X-API-KEY"
  },
  "StorageOptions": {
    "FileMountPath": "C:\\Test_1\\MountTest" //will be local directory for now (installed Python)
  },
  "BrunnhildeOptions": {
    "PathToBrunnhilde": "C:\\Users\\BrianMcEnroy\\AppData\\Local\\Programs\\Python\\Python314\\Lib\\site-packages\\brunnhilde.py",
    "PathToPython": "C:\\Users\\BrianMcEnroy\\AppData\\Local\\Programs\\Python\\Python314\\python.exe",
    "DirectorySeparator": "\\",
    "ObjectsFolder": "objects"
  },
  "ConnectionStrings": {
    "PreservationApiBaseUrl": "https://localhost:7228/"
  },
  "Preservation": {
    "Root": "https://localhost:7228"
  },
  //Leeds
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "enter tenant id",
    "ClientId": "enter client id",
    "Audience": "enter audience"
  },
  //Auth Token Provider
  "TokenProvider": {
    "TenantId": "enter tenant id",
    "ClientSecret": "enter client secret",
    "ClientId": "enter client id"
  }
}