{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Warning",
        "HealthChecks": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext} {CorrelationId}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Storage-AWS": {
    "Region": "eu-west-1"
  },
  "AwsStorage": {
    "DefaultWorkingBucket": "dlip-pres-dev-deposits",
    "S3HealthCheckKey": "checks/test.json"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "CallbackPath": "/idpresponse"
  },
  "DownstreamApi": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "user.read"
  },
  "IdentityService": {
    "Root": "https://id.library.leeds.ac.uk",
    "TimeoutMs": 5000,
    "ApiKeyHeader": "X-API-KEY",
    "AlternativeCollectionsContainer": "cc-test"
  },
  "MvpCatalogueApi": {
    "Root": "https://explore.library.leeds.ac.uk",
    "QueryTemplate": "/imu/utilities/getIIIFData.php?pid=",
    "TimeoutMs": 5000,
    "ApiKeyHeader": "X-API-KEY"
  }
}
