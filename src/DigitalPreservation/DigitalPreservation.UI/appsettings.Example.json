    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "Preservation": {
    "Root": "https://localhost:7228"
  },
  "Storage-AWS": {
    "Profile": "leeds",
    "Region": "eu-west-1"
  },
  "AwsStorage": {
    "DefaultWorkingBucket": "dlip-pres-dev-deposits",
    "S3HealthCheckKey": "checks/test.json"
  },
