{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "HealthChecks": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext} {CorrelationId}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Storage-AWS": {
    "Region": "eu-west-1"
  },
  "AwsStorage": {
    "DefaultWorkingBucket": "dev-deposits",
    "S3HealthCheckKey": "checks/test.json"
  },
  "ImportExport": {
    "ImportJobSqsQueueName": "importer-queue",
    "ExportJobSqsQueueName": "export-job-runner-queue"
  }
}
