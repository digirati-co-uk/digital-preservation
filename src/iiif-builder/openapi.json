    "components": {
        "securitySchemes": {
            "ApiKeyAuth": {
                "type": "apiKey",
                "name": "X-API-KEY",
                "in": "header"
            }
        },
        "parameters": {
            "identifierParam": {
                "name": "id",
                "in": "path",
                "description": "Identifier used in the service",
                "required": true,
                "schema": {
                    "type": "string",
                    "pattern": "^[a-z0-9]*[bcdfghjkmnpqrstvwxyz][0123456789bcdfghjkmnpqrstvwxyz]{7}$",
                    "example": "abcd1234"
                }
            },
            "irnParam": {
                "name": "irn",
                "in": "path",
                "description": "EMu Internal Resource Number (IRN) used in different modules of the EMu Application",
                "required": true,
                "schema": {
                    "type": "string",
                    "pattern": "^[0-9]+$",
                    "example": "765123"
                }
            },
            "queryParam": {
                "description": "Text used in a search for an identifier. The search is performed across the metadata associated with an identifier.",
                "in": "query",
                "name": "q",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "pageParam": {
                "description": "The number of the page to display. Determines how pagination of results works in conjunction with the per-page parameter (pp)",
                "in": "query",
                "name": "page",
                "required": false,
                "schema": {
                    "type": "integer",
                    "minimum": 1,
                    "default": 1
                }
            },
            "perPageParam": {
                "description": "The number of records to display on a page. Determines how pagination of results works in conjunction with the page parameter (p)",
                "in": "query",
                "name": "per_page",
                "required": false,
                "schema": {
                    "type": "integer",
                    "minimum": 1,
                    "default": 10
                }
            },
            "metaSchemaParam": {
                "description": "A parameter which identifies one or more of the metadata types used for identifiers",
                "in": "query",
                "name": "s",
                "required": false,
                "schema": {
                    "type": "string",
                    "enum": [
                        "title",
                        "desc",
                        "catirn",
                        "accirn",
                        "ptyirn",
                        "siteirn",
                        "epid",
                        "ludosid",
                        "doi",
                        "redirect",
                        "provenance"
                    ]
                }
            },
            "statusParam": {
                "description": "Limit the results of the query to identifiers with the given status. Only live and deleted are currently supported",
                "in": "query",
                "name": "status",
                "required": false,
                "schema": {
                    "type": "string",
                    "enum": [
                        "live",
                        "deleted"
                    ]
                }
            },
            "createdFromParam": {
                "description": "Limit the results of the query to identifiers created after the date specified",
                "in": "query",
                "name": "created_from",
                "required": false,
                "schema": {
                    "type": "string",
                    "format": "date"
                }
            },
            "createdToParam": {
                "description": "Limit the results of the query to identifiers created before the date specified",
                "in": "query",
                "name": "created_to",
                "required": false,
                "schema": {
                    "type": "string",
                    "format": "date"
                }
            },
            "updatedFromParam": {
                "description": "Limit the results of the query to identifiers updated after the date specified",
                "in": "query",
                "name": "updated_from",
                "required": false,
                "schema": {
                    "type": "string",
                    "format": "date"
                }
            },
            "updatedToParam": {
                "description": "Limit the results of the query to identifiers updated after the date specified",
                "in": "query",
                "name": "updated_to",
                "required": false,
                "schema": {
                    "type": "string",
                    "format": "date"
                }
            },
            "orderbyParam": {
                "description": "List identifiers in an order determined by the value of this field",
                "in": "query",
                "name": "orderby",
                "required": false,
                "schema": {
                    "type": "string",
                    "enum": [
                        "created",
                        "updated",
                        "noid"
                    ]
                }
            },
            "orderParam": {
                "description": "List identifiers in either ascending or descending order",
                "in": "query",
                "name": "order",
                "required": false,
                "schema": {
                    "type": "string",
                    "enum": [
                        "asc",
                        "desc"
                    ]
                }
            }
        },
        "schemas": {
            "Identifier": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "pattern": "^[a-z0-9]*[bcdfghjkmnpqrstvwxyz][0123456789bcdfghjkmnpqrstvwxyz]{7}$",
                        "description": "identifier",
                        "example": "abcd1234"
                    },
                    "title": {
                        "type": "string",
                        "description": "Title",
                        "example": "Catalogue of English furniture & woodwork (v.3)"
                    },
                    "description": {
                        "type": "string",
                        "description": "Description",
                        "example": "v. 1 Revised edition. First printed 1923.\\n\\nIncludes bibliographical references and indexes.\\n\\nv. 1. Gothic and early Tudor / by H. Clifford Smith -- v. 2. Late Tudor and early Stuart / by H. Clifford Smith -- v. 3. Late Stuart to Queen Anne / by Oliver Brackett -- v. 4. Georgian / by Ralph Edwards."
                    },
                    "created": {
                        "type": "string",
                        "description": "Created date",
                        "format": "date-time",
                        "example": "Fri, 19 Apr 2024 12:12:16 GMT"
                    },
                    "updated": {
                        "type": "string",
                        "description": "Last-modified date",
                        "format": "date-time",
                        "example": "Fri, 19 Apr 2024 12:32:16 GMT"
                    },
                    "status": {
                        "type": "string",
                        "description": "Status",
                        "enum": ["deleted", null],
                        "example": ""
                    },
                    "catirn": {
                        "type": "string",
                        "description": "EMu Catalogue IRN",
                        "example": "703276"
                    },
                    "accirn": {
                        "type": "string",
                        "description": "EMu Accession IRN",
                        "example": "720335"
                    },
                    "ptyirn": {
                        "type": "string",
                        "description": "EMu Party IRN",
                        "example": "78346"
                    },
                    "siteirn": {
                        "type": "string",
                        "description": "EMu Site IRN",
                        "example": "12345"
                    },
                    "epid": {
                        "type": "string",
                        "description": "EPrint ID",
                        "example": "20132"
                    },
                    "ludosid": {
                        "type": "string",
                        "description": "LUDOS ID",
                        "example": "10298"
                    },
                    "doi": {
                        "type": "string",
                        "description": "Digital Object Identifier (DOI)",
                        "example": "10078.2301928"
                    },
                    "repositoryuri": {
                        "type": "string",
                        "format": "uri",
                        "description": "Archival Group URI in repository",
                        "example": "https://repository.library.leeds.ac.uk/object/abcd1234"
                    },
                    "manifesturi": {
                        "type": "string",
                        "format": "uri",
                        "description": "IIIF Manifest URI",
                        "example": "https://iiif.library.leeds.ac.uk/manifests/abcd1234"
                    },
                    "catalogueapiuri": {
                        "type": "string",
                        "format": "uri",
                        "description": "Catalogue API URI",
                        "example": "https://catalogue-api.library.leeds.ac.uk/id/abcd1234"
                    },
                    "redirect": {
                        "type": "string",
                        "format": "uri",
                        "description": "Redirection URL"
                    },
                    "provenance": {
                        "type": "string",
                        "description": "Provenance"
                    }
                },
                "xml": {
                    "name": "identifier"
                }
            },
            "IdentifierMetadata": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string",
                        "description": "Title",
                        "example": "Catalogue of English furniture & woodwork (v.3)"
                    },
                    "description": {
                        "type": "string",
                        "description": "Description",
                        "example": "v. 1 Revised edition. First printed 1923.\\n\\nIncludes bibliographical references and indexes.\\n\\nv. 1. Gothic and early Tudor / by H. Clifford Smith -- v. 2. Late Tudor and early Stuart / by H. Clifford Smith -- v. 3. Late Stuart to Queen Anne / by Oliver Brackett -- v. 4. Georgian / by Ralph Edwards."
                    },
                    "catirn": {
                        "type": "string",
                        "description": "EMu Catalogue IRN",
                        "example": "703276"
                    },
                    "accirn": {
                        "type": "string",
                        "description": "EMu Accession IRN",
                        "example": "720335"
                    },
                    "ptyirn": {
                        "type": "string",
                        "description": "EMu Party IRN",
                        "example": "78346"
                    },
                    "siteirn": {
                        "type": "string",
                        "description": "EMu Site IRN",
                        "example": "12345"
                    },
                    "epid": {
                        "type": "string",
                        "description": "EPrint ID",
                        "example": "20132"
                    },
                    "ludosid": {
                        "type": "string",
                        "description": "LUDOS ID",
                        "example": "10298"
                    },
                    "doi": {
                        "type": "string",
                        "description": "Digital Object Identifier (DOI)",
                        "example": "10078.2301928"
                    },
                    "repositoryuri": {
                        "type": "string",
                        "format": "uri",
                        "description": "Archival Group URI in repository",
                        "example": "https://repository.library.leeds.ac.uk/object/abcd1234"
                    },
                    "manifesturi": {
                        "type": "string",
                        "format": "uri",
                        "description": "IIIF Manifest URI",
                        "example": "https://iiif.library.leeds.ac.uk/manifests/abcd1234"
                    },
                    "catalogueapiuri": {
                        "type": "string",
