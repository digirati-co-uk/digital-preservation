                "tags": ["Identifiers"],
                "summary": "Get information about an identifier using an EMu Site IRN",
                "description": "Returns a description of a single identifier",
                "operationId": "get_identifier_by_siteirn",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/irnParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Identifier"
                                }
                            }
                        }
                    },
                    "401": {
