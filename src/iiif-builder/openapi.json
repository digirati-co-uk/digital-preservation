{
    "openapi": "3.0.2",
    "info": {
        "title": "University of Leeds Identifier Service",
        "description": "This is the service responsible for managing unique identifiers for digital objects in the University of Leeds libraries.",
        "termsOfService": "https://www.leeds.ac.uk/termsandconditions",
        "contact": {
            "email": "p.l.edwards@leeds.ac.uk"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "externalDocs": {
        "description": "Find out more about identifiers at the University of Leeds",
        "url": "https://id.library.leeds.ac.uk/"
    },
    "servers": [
        {
            "description": "Production server",
            "url": "https://id.library.leeds.ac.uk/api/v1"
        },
        {
            "description": "Development server",
            "url": "https://dev-id.library.leeds.ac.uk/api/v1"
        }
    ],
    "tags": [],
    "paths": {
        "/ids": {
            "get": {
                "tags": ["Identifiers"],
                "summary": "Lists existing identifiers",
                "description": "Returns a paginated list of identifiers in JSON format. Also accepts query parameters to allow searching identifiers.",
                "operationId": "query_identifier",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/queryParam"
                    },
                    {
                        "$ref": "#/components/parameters/metaSchemaParam"
                    },
                    {
                        "$ref": "#/components/parameters/statusParam"
                    },
                    {
                        "$ref": "#/components/parameters/pageParam"
                    },
                    {
                        "$ref": "#/components/parameters/perPageParam"
                    },
                    {
                        "$ref": "#/components/parameters/createdFromParam"
                    },
                    {
                        "$ref": "#/components/parameters/createdToParam"
                    },
                    {
                        "$ref": "#/components/parameters/updatedFromParam"
                    },
                    {
                        "$ref": "#/components/parameters/updatedToParam"
                    },
                    {
                        "$ref": "#/components/parameters/orderbyParam"
                    },
                    {
                        "$ref": "#/components/parameters/orderParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "pagination": {
                                            "$ref": "#/components/schemas/Pagination"
                                        },
                                        "results": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Identifier"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorised",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorisedResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ForbiddenResponse"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": ["Identifiers"],
                "summary": "Mint a new identifier with the service",
                "operationId": "create_identifier",
                "requestBody": {
                    "description": "A new identifier can be requested from the service, and metadata attched to the identifier here",
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/IdentifierMetadata"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Identifier"
                                    }
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "identifier already exists",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Identifier"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorised",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorisedResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ForbiddenResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/ids/{id}": {
            "get": {
                "tags": ["Identifiers"],
                "summary": "Get information about an identifier",
                "description": "Returns a description of a single identifier",
                "operationId": "get_identifier",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/identifierParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Identifier"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorised",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorisedResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ForbiddenResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Identifier not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/NotFoundResponse"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": ["Identifiers"],
                "summary": "Update an identifier",
                "operationId": "describe_identifier",
                "description": "Updates the metadata associated with an existing identifier",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/identifierParam"
                    }
                ],
                "requestBody": {
                    "description": "Metadata for an identifier can be updated by using new values against the existing schema. Metadata is deleted by updating the metadata key with an empty string.",
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/IdentifierMetadata"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Identifier"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorised",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorisedResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ForbiddenResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Identifier not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/NotFoundResponse"
                                }
                            }
                        }
                    }
                }
            },
            "patch": {
                "tags": ["Identifiers"],
                "summary": "Mark an identifier as DELETED",
                "operationId": "delete_identifier",
                "description": "Updates the metadata associated with an existing identifier and marks its status as deleted",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/identifierParam"
                    }
                ],
                "requestBody": {
                    "description": "Deletion of an identifier should always be accompanied by a reason, and optionally by a URL corresponding to a new location for the resource represented by the identifier",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DeletionMetadata"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Identifier"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorised",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorisedResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ForbiddenResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Identifier not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/NotFoundResponse"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": ["Identifiers"],
                "summary": "Permanently delete an identifier and all metadata associated with it",
                "operationId": "shred_identifier",
                "description": "Removes all data for the identifier from the Noid and NoidMeta database tables",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/identifierParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SuccessResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorised",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorisedResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ForbiddenResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Identifier not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/NotFoundResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
